{
  "workbench.colorTheme": "Tokyo City Darker",
  "editor.fontFamily": "'JetBrains Mono', 'monospace', monospace",
  "window.zoomLevel": 2,
  "window.menuBarVisibility": "toggle",
  "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
  "vscode-neovim.useCtrlKeysForInsertMode": false,
  "vscode-neovim.useCtrlKeysForNormalMode": false,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "discord.removeRemoteRepository": true,
  "discord.removeTimestamp": true,
  "discord.swapBigAndSmallImage": true,
  "discord.removeLowerDetails": true
}
